    "def dudx_BD(u, dx)\n",
    "    ''' Return du/dx using backward-difference quotient'''\n",
    "    dudx = (u[1:-1] - u[:-2]) / dx\n",
    "    return dudx\n",
    "\n",
    "def dudx_CD(u, dx)\n",
    "    ''' Return du/dx using central-difference quotient'''\n",
    "    dudx = (u[2:] - u[:-2]) / (2 * dx)\n",
    "    return dudx\n",
    "\n"
   ]
  }
